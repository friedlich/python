!function(){    /**
 * @file 尾部内联js
 * @author wutao06
 */

T.lang.eventCenter.addEventListener('pack.button.hide', function () {
            $('.pack-pay').css('display', 'inline-block');
    });

var Reader = require('wkview:widget/common_toc_reader/reader_txt/index.js');
new Reader({
    doc_id: 'cbb4af8b783e0912a3162a89',
    container: $('.reader-container .reader-container-inner .bd'),
    el: T.dom.query('.reader-container')[0],
    totalPageNum: parseInt('4', 10),
    pageCount: parseInt('4', 10),
    toPage: parseInt('1', 10),
    shareElId: 'shareMore-3'
});
require.async(['wkcommon:widget/lib/tangram/base/base.js', 'wkview:widget/ui/view/doc_share/doc_share.js', 'wkview:widget/ui/view/down_wangpan/down_doc.js'], function (T, docShare, Wangpan) {
    var wangpan = Wangpan.downDocCreate();
    T.lang.eventCenter.addEventListener('Reader.zoomChange', function (e) {
        var docBodyEl = T.dom.query('#bd .body')[0];
        var bdWrap = T.dom.query('#bd .bd-wrap')[0];
        var crubmsWrap = T.dom.query('#bd .crubms-wrap');
        if (crubmsWrap.length > 1) {
            crubmsWrap = crubmsWrap[0];
            crubmsWrap.style.width = docBodyEl.offsetWidth + 'px';
        }
        bdWrap.style.minWidth = docBodyEl.offsetWidth + 'px';
    });
    T.lang.eventCenter.addEventListener('wangpan.doc', function (e, data) {
        data = data || {};
        var from = data.from || false;
        if (from === 'payDialog') {
            wangpan.getFile();
        }
        else {
            wangpan.start();
        }
    });

    // 分享
    var share = docShare.shareToCreate({
        el: T.dom.q('reader-tools-share')[0],
        view: 'viewReader-3',
        copy: 'copyCode-3',
        panel: 'panel-3',
        closePanel: 'closePanel-3',
        toggleSize: ['normalSize-3', 'bigSize-3'],
        codeText: 'readerCode-3',
        copyResult: 'copyResult-3'
    });
    // 显示更多分享
    share.show();

});;
}();
